    "project/TastyMiMaFilters.scala",
    ".github/workflows/**",
    "compiler/src/dotty/tools/dotc/core/**",
    "compiler/src/dotty/tools/dotc/Compiler.scala",
    "compiler/src/dotty/tools/dotc/Run.scala",
